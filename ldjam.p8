pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
collideableobjects = {}

map = {
	sizex = 20,
	sizey = 20,
}

cam = {
	x = 0,
	y = 0
}

player = {
	x = 64,
	y = 64,
	dx = 0,
	dy = 0,
	state = 0,
	sprite = 0
}

dialog = {
	script = nil,
	pos = 0,
	guy1 = {
		"hi there!",
		"how are you?",
		"...",
		"oh.. i'm sorry to hear that"
	}
}

function _update()
	if (player.state == 0) then
		handleplayermovement()
	elseif (player.state == 1) then
		handledialog()
	end
end

function _draw()
	cls()
	camera(cam.x ,cam.y)
	drawmap()
	spr(player.sprite,player.x,player.y)
	print(flr(player.x / 8)..","..flr(player.y / 8))
	if (player.state == 1) then
		drawdiabox()
	end
end

function handleplayermovement()
	player.dx = 0
	player.dy = 0
	handlecontrols()
	player.x += player.dx
	player.y += player.dy
	if (positioniscollideable()) then
 	player.x -= player.dx
 	player.y -= player.dy
	end
	handlemoveplayeraccrossmap()
	cam.x = player.x - 63
	cam.y = player.y - 63
end

function handlemoveplayeraccrossmap()
	if player.x < 0 then
		player.x += map.sizex * 8
	end
	if player.y < 0 then
		player.y += map.sizey * 8
	end
	if player.x > map.sizex * 8 then
		player.x -= map.sizex * 8
	end
	if player.y > map.sizey * 8 then
		player.y -= map.sizey * 8
	end
end

function handlecontrols()
	if (btn(0)) then
		player.dx = -1
	end
	if (btn(1)) then
		player.dx = 1
	end
	if (btn(2)) then
		player.dy = -1
	end
	if (btn(3)) then
		player.dy = 1
	end
	if (btnp(4)) then
		dialog.script = dialog.guy1
		dialog.pos = 1
		player.state = 1
	end
end

function positioniscollideable()
	mapvalue = mget(flr((player.x+4) / 8),flr((player.y+4) / 8))
	return fget(mapvalue, 1)
end

function drawmap()
	mapdraw(0, 0, map.sizex * -8, map.sizey * -8, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex * -8, map.sizey *  0, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex * -8, map.sizey *  8, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  0, map.sizey * -8, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  0, map.sizey *  0, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  0, map.sizey *  8, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  8, map.sizey * -8, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  8, map.sizey *  0, map.sizex,map.sizey)
	mapdraw(0, 0, map.sizex *  8, map.sizey *  8, map.sizex,map.sizey)
end

-- dialog system --

function handledialog()
	if (btnp(4)) then
		if (dialog.pos < count(dialog.script)) then
			dialog.pos += 1
		else
			player.state = 0
		end
	end
end

function drawdiabox()
	camera(0,0)
	rectfill(0, 95, 127, 127, 0)
	rect(0, 96, 127, 127, 15)
	print(dialog.script[dialog.pos], 2, 98)
end
